<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="kamera, ip kamera, ahd kamera, termal kamera, nvr kayıt cihazı,kars güvenlik kamera hizmetleri,kars güvenlik kamera, kars ip kamera, kars ahd kamera, kars en uygun güvenlik kamera sistemleri">
    <meta name="keywords" content="kamera, ip kamera, ahd kamera, termal kamera, nvr kayıt cihazı,kars güvenlik kamera hizmetleri,kars güvenlik kamera, kars ip kamera, kars ahd kamera, kars en uygun güvenlik kamera sistemleri">
    <title>@ViewData["Title"] - Admin Batuhan Güvenlik Kamera Sistemleri</title>
    <link href="https://cdn.jsdelivr.net/npm/simple-datatables@7.1.2/dist/style.min.css" rel="stylesheet" />
    <link href="/admin/admin/css/styles.css" rel="stylesheet" />
    <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->
        <a class="navbar-brand ps-3" href="/Admin">Admin</a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
            <div class="input-group">
                <input class="form-control" type="text" placeholder="Search for..." aria-label="Search for..." aria-describedby="btnNavbarSearch" />
                <button class="btn btn-primary" id="btnNavbarSearch" type="button"><i class="fas fa-search"></i></button>
            </div>
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="/Admin/Main/MyProfile">Profilim</a></li>
                    <li><a class="dropdown-item" href="/Admin/Main/MyPasswordChange">Şifre Değiştir</a></li>
                    <li><hr class="dropdown-divider" /></li>
                    <li><a class="dropdown-item" href="/Admin/Main/SignOut">Çıkış</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <!-- Sol Menü-->
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <a class="nav-link" href="/Admin">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-house"></i></div>
                            Anasayfa
                        </a>
                        <a class="nav-link" href="/Admin/AppUsers">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-user"></i></div>
                            Kullanıcılar
                        </a>
                        <a class="nav-link" href="/Admin/Abouts">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-address-book"></i></div>
                            Hakkımızda
                        </a>
                        <a class="nav-link" href="/Admin/Galeries">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-list"></i></div>
                            Çalışmalar
                        </a>
                        <a class="nav-link" href="/Admin/Services">
                            <div class="sb-nav-link-icon"><i class="fa-brands fa-servicestack"></i></div>
                            Hizmetler
                        </a>
                        <a class="nav-link" href="/Admin/Prices">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-money-bill-wave"></i></div>
                            Fiyatlar
                        </a>
                        <a class="nav-link" href="/Admin/Comments">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-comments"></i></div>
                            Yorumlar
                        </a>
                        <a class="nav-link" href="/Admin/Contacts">
                            <div class="sb-nav-link-icon"><i class="fa-solid fa-location-dot"></i></div>
                            İletişim
                        </a>
                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div>Batuhan Güvenlik Kamera Hizmetleri</div>                 
                    <div class="small">Hoşgeldiniz: <span>@Context.User.Identity.Name</span></div>
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main role="main">
                <div class="container-fluid px-4">
                    <h1 class="mt-4 mb-4">@ViewData["Title"]</h1>
                    @RenderBody()
                </div>
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid px-4">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy;@DateTime.Now.Year <strong class="px-1 sitename">Batuhan Güvenlik Kamera Sistemleri</strong> Tüm Hakları Saklıdır.<a href="#">&nbsp; Velat BARAN Tarafından Tasarlandı.</a></div>                  
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="/admin/admin/js/scripts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
    <script src="/admin/admin/assets/demo/chart-area-demo.js"></script>
    <script src="/admin/admin/assets/demo/chart-bar-demo.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/simple-datatables@7.1.2/dist/umd/simple-datatables.min.js" crossorigin="anonymous"></script>
    <script src="/admin/admin/js/datatables-simple-demo.js"></script>
</body>
</html>
